<!DOCTYPE html>
<html>
<head>
  <title>Jungle</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

  <%= render 'layouts/top_nav' %>
<div class="log_links pull-right">
<% if current_user%>
  <p class="login_logout">
   <%= link_to "Logout", '/logout' %>
   </p>

  <% else%>

   <p class="login_logout"> <%= link_to "Login", '/login' %>
    <%= link_to "Register", '/signup' %>
    </p>

  <% end %>
  </div>



  <main class="container">
    <%= yield %>
  </main>

  <%= yield :end_of_body %>
</body>
</html>
